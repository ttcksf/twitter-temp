{
  "name": "Twitter投稿テンプレート",
  "description": "Twitter投稿テンプレートです。",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://x.com/*"],
      "run_at": "document_end",
      "js": ["./main.js"]
    }
  ],
  "icons": {
    "16": "./images/icon16.png",
    "32": "./images/icon32.png",
    "48": "./images/icon48.png",
    "128": "./images/icon128.png"
  }
}
